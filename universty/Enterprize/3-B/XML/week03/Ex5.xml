<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 객체의 이름은 소문자 클래스의 이름은 대문자 -->
	<!-- property는 set방식을 이용한 bean 주입방식 -->
	<bean name="collectionEx" class="week03.CollectionEx">
		<property name="deviations">
			<!-- list 타입 내에 있는 객체들이 동일한 타입일 경우 value-type으로 동시 선언 가능 -> 리스트의 제네릭 타입 선언 시 -->
			<list value-type="java.lang.Double">
				<value>0.2</value>
				<value>0.3</value>
				<value>0.4</value>
			</list>
		</property>
	</bean>
</beans>
