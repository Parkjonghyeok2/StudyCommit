<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 의존관계 자동설정 법 constructor 에 있는 매개변수들을 자동으로 자바 빈 설정을 해주고 그 설정은 하단에 무조건 선언 해줘야한다 -->
	<bean name="autoDependecy" class="week03.AutoDependecy"
		autowire="constructor" />

	<bean name="smsSend" class="common.SmsSender" />
	<bean name="collectionEx" class="week03.CollectionEx" />
</beans>
